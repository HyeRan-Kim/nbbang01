<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="nbbang.mybatis.mapper.admin">
	<select id="selectAllMember" parameterType="Map" resultType="member">
		SELECT * FROM 
		(SELECT T.*,ROWNUM R FROM (SELECT * FROM member ORDER BY registdate DESC) T) 
		WHERE R BETWEEN #{start} AND #{end}
	</select>
	<select id="getTotalRowCount" parameterType="Map" resultType="int">
		SELECT COUNT(*) FROM member
	</select>
</mapper>